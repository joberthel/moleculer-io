{
    "name": "moleculer-io",
    "version": "1.1.3",
    "description": "An API GateWay service for moleculerjs using socket.io",
    "main": "lib/index.js",
    "directories": {
        "example": "examples"
    },
    "scripts": {
        "dev": "babel src -d lib --watch",
        "example": "npm install && node examples/simple/index.js",
        "test": "ava",
        "build": "babel -d lib/ src/",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/joberthel/moleculer-io.git"
    },
    "keywords": [
        "socket.io",
        "moleculer",
        "microservice",
        "API Gateway"
    ],
    "author": "tiaod",
    "license": "MIT",
    "dependencies": {
        "chalk": "^4.1.0",
        "lodash": "^4.17.21",
        "socket.io": "^4.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.10",
        "@babel/core": "^7.13.13",
        "@babel/preset-env": "^7.13.12",
        "ava": "^3.15.0",
        "express": "^4.17.1",
        "fs-extra": "^9.1.0",
        "moleculer": "^0.14.12",
        "moleculer-web": "^0.9.1",
        "socket.io-client": "^4.0.0",
        "socket.io-redis": "^6.1.0",
        "winston": "^3.3.3"
    }
}
